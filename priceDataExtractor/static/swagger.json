    "/prices": {
      "get": {
        "summary": "Get Azure prices",
        "description": "Retrieve all Azure prices, optionally filtered by service name and region.",
        "parameters": [
          {
            "name": "serviceName",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "The name of the Azure service to filter by."
          },
          {
            "name": "region",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "The region to filter by."
          }
        ],
        "responses": {
          "200": {
            "description": "A list of prices",
            "schema": {
              "type": "array",
              "items": {
                "type": "object"
              }
            }
          }
        }
      }
